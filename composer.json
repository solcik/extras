{
	"name": "solcik/extras",
	"description": "",
	"license": "MIT",
	"type": "library",
	"homepage": "https://github.com/solcik/extras",
	"authors": [
		{
			"name": "David Solc",
			"email": "solcik@gmail.com",
			"homepage": "https://solc.dev"
		}
	],
	"require": {
		"php": "^7.4.0"
	},
	"require-dev": {
		"brick/date-time": "^0.2.0",
		"brick/math": "^0.8.11",
		"contributte/event-dispatcher-extra": "dev-master",
		"doctrine/common": "^2.12.0",
		"doctrine/dbal": "^2.10.1",
		"firebase/php-jwt": "^5.0.0",
		"guzzlehttp/guzzle": "^6.5.2",
		"jakub-onderka/php-console-highlighter": "^0.4.0",
		"jakub-onderka/php-parallel-lint": "^1.0.0",
		"jean85/pretty-package-versions": "^1.2.0",
		"laminas/laminas-diactoros": "^2.2.2",
		"laminas/laminas-httphandlerrunner": "^1.1.0",
		"latte/latte": "^2.6.1",
		"neomerx/json-api": "^4.0.0",
		"nette/application": "^3.0.4",
		"nette/bootstrap": "^3.0.1",
		"nette/caching": "^3.0.1",
		"nette/di": "^3.0.3",
		"nette/forms": "^3.0.3",
		"nette/reflection": "^2.4.2",
		"nette/schema": "^1.0.2",
		"nette/security": "^3.0.3",
		"nette/tester": "^2.3.1",
		"nette/utils": "^3.1.0",
		"nikic/fast-route": "^1.3.0",
		"pepakriz/phpstan-exception-rules": "^0.10.1",
		"php-http/guzzle6-adapter": "^2.0.1",
		"phpstan/phpstan": "^0.12.8",
		"phpstan/phpstan-deprecation-rules": "^0.12.2",
		"phpstan/phpstan-strict-rules": "^0.12.2",
		"psr/http-message": "^1.0.1",
		"psr/http-server-handler": "^1.0.1",
		"psr/http-server-middleware": "^1.0.1",
		"ramsey/uuid": "^3.9.2",
		"roave/security-advisories": "dev-master",
		"sentry/sentry": "^2.3.1",
		"symfony/event-dispatcher": "^5.0.3",
		"symfony/translation": "^5.0.3",
		"symplify/easy-coding-standard": "^7.2.2"
	},
	"autoload": {
		"psr-4": {
			"Solcik\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Solcik\\Test\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"scripts": {
		"qa": [
			"@lint",
			"@cs",
			"@phpstan"
		],
		"lint": [
			"parallel-lint ./src ./tests"
		],
		"cs": [
			"ecs check ./src ./tests"
		],
		"csfix": [
			"ecs check ./src ./tests --fix"
		],
		"tests": [
			"tester -s -p php --colors 1 -C ./tests/cases"
		],
		"coverage": [
			"tester -s -p phpdbg --colors 1 -C --coverage ./coverage.xml --coverage-src ./src ./tests/cases"
		],
		"phpstan": [
			"phpstan analyse"
		]
	}
}
